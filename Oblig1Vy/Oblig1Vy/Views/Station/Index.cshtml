@model IEnumerable<Oblig1Vy.Model.ViewModels.StationVm>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Opprett ny stasjon", "AddStation", "Station")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StationName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StationName)
        </td>
        <td>
            @Html.ActionLink("Endre", "Edit", new { id=item.StationId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StationId })
        </td>
    </tr>
}

</table>
